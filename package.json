{
  "name": "social_cops_submission",
  "version": "1.0.0",
  "description": "a application for the submission of social cops internship task",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "yarn dev:start",
    "dev:start": "nodemon --ignore lib --exec babel-node index.js",
    "dev:wds": "webpack-dev-server --progress",
    "prod:build": "rimraf lib && babel / -d lib --ignore .test.js",
    "prod:start": "cross-env NODE_ENV=production nodemon start lib/",
    "test": "yarn lint && yarn mocha",
    "lint": "eslint *.js --ext .js,.jsx",
    "mocha": "cross-env NODE_ENV=test nyc mocha test/*.test.js --exit",
    "cover": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "keywords": [
    "social-cops"
  ],
  "author": "Piyush Vijay",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^1.0.3",
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^5.0.1",
    "ejs": "~2.5.7",
    "express": "~4.15.5",
    "express-session": "^1.15.6",
    "jsonwebtoken": "^8.2.0",
    "morgan": "~1.9.0",
    "mysql": "^2.15.0",
    "request": "^2.85.0",
    "serve-favicon": "~2.4.5",
    "sharp": "^0.20.1",
    "supertest": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-flow-react-proptypes": "^20.1.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.4",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.45.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.7.0",
    "fetch-mock": "^6.0.1",
    "flow-bin": "^0.66.0",
    "istanbul": "^0.4.5",
    "jest": "^22.4.0",
    "mocha": "^4.0.0",
    "nodemon": "^1.15.1",
    "nyc": "^11.6.0",
    "rimraf": "^2.6.2",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
